{
  "name": "symbol-sync",
  "version": "1.0.0",
  "description": "Quickly turn your Figma icons into React components with SymbolSync - your creative shortcut to coding",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "tsx ./src/index.ts",
    "build": "tsup ./src/index.ts --format cjs,esm --dts",
    "test": "vitest",
    "prepare": "husky"
  },
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "18.6.0",
    "@commitlint/config-conventional": "18.6.0",
    "@lucasaugustsof/eslint-config": "1.0.1",
    "@lucasaugustsof/ts-config": "0.1.0",
    "@types/node": "20.11.5",
    "@types/prompts": "2.4.9",
    "@types/react": "18.2.48",
    "eslint": "8.56.0",
    "husky": "9.0.7",
    "tsx": "4.7.0",
    "typescript": "5.3.3",
    "vitest": "1.2.1"
  },
  "dependencies": {
    "@svgr/core": "8.1.0",
    "@svgr/plugin-jsx": "8.1.0",
    "@svgr/plugin-prettier": "8.1.0",
    "@svgr/plugin-svgo": "8.1.0",
    "camelcase": "8.0.0",
    "chalk": "5.3.0",
    "commander": "11.1.0",
    "dotenv": "16.3.2",
    "ora": "8.0.1",
    "prompts": "2.4.2",
    "tsup": "8.0.1"
  }
}
